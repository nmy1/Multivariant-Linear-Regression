#include <iostream>
#include <fstream>
using namespace std;

#define MAX 5000
double x[MAX][MAX];
double y[MAX];
int n, m;
double alpha = 0.01; //learn rating
double O[MAX], Otemp[MAX];
double d[MAX];
double oldO[MAX];
int flag = 1;
double sum = 0; //actural value

void input()
{
    ifstream in("LGdata.txt");
    in >> n; //number of variables
    in >> m; //the number of data of each variable
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < m; j++)
        {
            in >> x[i][j];
        }
    }
    for (int i = 0; i < m; i++)
    {
        in >> y[i];
    }
    in.close();
}

void reset()
{
    for (int i = 0; i < n; i++)
    {
        O[i] = 0;
        Otemp[i] = 100;
        d[i] = 0;
        oldO[i] = 100;
    }
}

void calculate()
{
    do
    {
        for (int i = 0; i <= n; i++)
        {
            oldO[i] = O[i];
        }
        for (int i = 0; i < m; i++)
        {
            sum = O[0];
            for (int j = 1; j <= n; j++)
            {
                sum += O[j] * x[j - 1][i];
            }
            d[0] += sum - y[i];
        }
        d[0] /= m;
        Otemp[0] = O[0] - alpha * d[0];
        for (int i = 1; i <= n; i++)
        {
            for (int p = 0; p < m; p++)
            {
                sum = O[0];
                for (int j = 1; j <= n; j++)
                {
                    sum += O[j] * x[j - 1][p];
                }
                d[i] += (sum - y[p]) * x[i - 1][p];
            }
            d[i] /= m;
            Otemp[i] = O[i] - alpha * d[i];
        }
        for (int i = 0; i <= n; i++)
        {
            O[i] = Otemp[i];
        }
        flag = 1;
        for (int i = 0; i < n; i++)
        {
            if (O[i] != oldO[i])
            {
                flag = 0;
                break;
            }
        }
        for (int i = 0; i < n + 1; i++)
        {
            cout << O[i] << ' ';
        }
    } while (flag != 1);
}

void output()
{
    cout << "The final result of coefficient:" << endl;
    for (int i = 0; i < n + 1; i++)
    {
        cout << O[i] << ' ';
    }
}

int main()
{
    input();
    reset();
    calculate();
    output();
    return 0;
}
